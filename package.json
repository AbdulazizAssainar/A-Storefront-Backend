{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "## Getting Started",
  "main": "server.ts",
  "scripts": {
    "start": "npm run build && db-migrate up && node build/server.js",
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "lint": "eslint . --ext .ts --fix --quiet",
    "clean": "npm run prettier && npm run lint",
    "jasmine": "npm run build &&  jasmine-ts",
    "build": "npx tsc",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "clear_test": "db-migrate db:drop udacity_test",
    "test": "npm run clear_test && db-migrate db:create udacity_test && set ENV=test && db-migrate --env test up && npm run jasmine && npm run clear_test",
    "tsc": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jquery": "^3.5.14",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "express": "^4.18.2",
    "global": "^4.4.0",
    "jsonwebtoken": "^9.0.0",
    "node-json-db": "^2.1.3",
    "pg": "^8.5.1",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/pg": "^7.14.7",
    "cross-env": "^7.0.3",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.11",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "supertest": "^6.3.3",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  },
  "repository": "git+https://github.com/AbdulazizAssainar/A-Storefront-Backend.git",
  "bugs": {
    "url": "https://github.com/AbdulazizAssainar/A-Storefront-Backend/issues"
  },
  "homepage": "https://github.com/AbdulazizAssainar/A-Storefront-Backend#readme"
}
